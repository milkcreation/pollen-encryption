{
  "name": "pollen-solutions/encryption",
  "description": "Pollen Encryption Component",
  "homepage": "https://www.presstify.com/pollen-solutions/encryption/",
  "keywords": [
    "pollen-solutions",
    "component",
    "encrypt",
    "decrypt"
  ],
  "time": "2021-02-17",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.0",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "pollen-solutions/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Encryption\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Encryption\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
